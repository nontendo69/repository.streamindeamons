<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.sdwizard" name="Streamin Deamons Wizard" version="0.1.1" provider-name="nontendo69">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Streamin Deamons Wizard</summary>
    <description lang="en">Streamin Deamons Wizard allows you to get access to the latest Streamin Deamons Custom Builds.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.streamindeamons" name="Streamin Deamons Repository" version="1.0" provider-name="nontendo69">
    <extension point="xbmc.addon.repository" name="Streamin Deamons Repositor">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/nontendo69/Addons/master/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/nontendo69/Addons/master/addons.xml.md5</checksum>
            <datadir zip="true">https://github.com/nontendo69/Addons/raw/master/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Streamin Deamons Repository</summary>
        <description>Download our Build!</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
